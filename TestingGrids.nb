Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"MrootMat", "[", "NN_", "]"}], ":=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"i", "\[Equal]", "j"}], ",", 
       SqrtBox[
        SubscriptBox["m", "i"]], ",", "0"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", "NN"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "1", ",", "NN"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Umat", "[", "NN_", "]"}], ":=", 
   RowBox[{"Simplify", "[", 
    RowBox[{
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"Which", "[", 
        RowBox[{
         RowBox[{"i", ">", 
          RowBox[{"n", "+", "1"}]}], ",", "0", ",", 
         RowBox[{"n", "==", "NN"}], ",", 
         FractionBox[
          SqrtBox[
           SubscriptBox["m", "i"]], 
          SqrtBox[
           RowBox[{"Sum", "[", 
            RowBox[{
             SubscriptBox["m", "j"], ",", 
             RowBox[{"{", 
              RowBox[{"j", ",", "1", ",", "NN"}], "}"}]}], "]"}]]], ",", 
         RowBox[{
          RowBox[{"n", "\[GreaterEqual]", "i"}], "&&", 
          RowBox[{"n", "<", "NN"}]}], ",", 
         FractionBox[
          RowBox[{"(", 
           SqrtBox[
            RowBox[{
             SubscriptBox["m", 
              RowBox[{"n", "+", "1"}]], 
             SubscriptBox["m", "i"]}]], ")"}], 
          RowBox[{"(", 
           RowBox[{
            SqrtBox[
             RowBox[{"Sum", "[", 
              RowBox[{
               SubscriptBox["m", "j"], ",", 
               RowBox[{"{", 
                RowBox[{"j", ",", "1", ",", 
                 RowBox[{"n", "+", "1"}]}], "}"}]}], "]"}]], 
            SqrtBox[
             RowBox[{"Sum", "[", 
              RowBox[{
               SubscriptBox["m", "j"], ",", 
               RowBox[{"{", 
                RowBox[{"j", ",", "1", ",", "n"}], "}"}]}], "]"}]]}], ")"}]], 
         ",", 
         RowBox[{
          RowBox[{"i", "==", 
           RowBox[{"n", "+", "1"}]}], "&&", 
          RowBox[{"n", "<", "NN"}]}], ",", 
         FractionBox[
          RowBox[{"-", 
           SqrtBox[
            RowBox[{"Sum", "[", 
             RowBox[{
              SubscriptBox["m", "j"], ",", 
              RowBox[{"{", 
               RowBox[{"j", ",", "1", ",", "n"}], "}"}]}], "]"}]]}], 
          SqrtBox[
           RowBox[{"Sum", "[", 
            RowBox[{
             SubscriptBox["m", "j"], ",", 
             RowBox[{"{", 
              RowBox[{"j", ",", "1", ",", 
               RowBox[{"n", "+", "1"}]}], "}"}]}], "]"}]]]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"n", ",", "1", ",", "NN"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", "NN"}], "}"}]}], "]"}], ",", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        SubscriptBox["m", "i"], ">", "0"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", "NN"}], "}"}]}], "]"}]}], "]"}]}], 
  ";"}]}], "Input",
 PageWidth->PaperWidth,
 CellChangeTimes->{{3.571852752962878*^9, 3.571852774372019*^9}, {
  3.5718530114540653`*^9, 3.571853036105225*^9}, {3.571853097451919*^9, 
