Cell[BoxData[
 RowBox[{
  RowBox[{"xminusSinxGrid", "[", 
   RowBox[{"x1_", ",", "x2_", ",", "NPTS_", ",", "R0_", ",", "R_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"\[CapitalDelta]x", ",", "xScale", ",", "res"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"\[CapitalDelta]x", "=", 
      RowBox[{"(", 
       RowBox[{"x2", "-", "x1"}], ")"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"res", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"x1", "+", 
         RowBox[{
          RowBox[{"(", 
           FractionBox[
            RowBox[{"f", "[", 
             RowBox[{
              RowBox[{"Exp", "[", 
               RowBox[{"Max", "[", 
                RowBox[{
                 FractionBox["R0", "R"], ",", ".75"}], "]"}], "]"}], ",", 
              RowBox[{"(", 
               RowBox[{"(", 
                FractionBox[
                 RowBox[{"i", "-", "1"}], 
                 RowBox[{"NPTS", "-", "1"}]], ")"}], ")"}]}], "]"}], 
            RowBox[{"f", "[", 
             RowBox[{
              RowBox[{"Exp", "[", 
               RowBox[{"Max", "[", 
                RowBox[{
                 FractionBox["R0", "R"], ",", ".75"}], "]"}], "]"}], ",", 
              "1"}], "]"}]], ")"}], "\[CapitalDelta]x"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "NPTS"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "res"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
